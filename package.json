{
  "name": "Twiterian",
  "version": "0.1.2",
  "description": "Tweet with images at popup window.",
  "author": "Hexin",
  "license": "MIT",
  "repository": "https://github.com/hexium310/Twiterian",
  "scripts": {
    "build": "run-s lint clean:release copy:manifest && webpack --mode production",
    "build:dev": "webpack --mode development && cpx src/manifest.json dist/",
    "clean": "run-p clean:*",
    "clean:dev": "rimraf dist/",
    "clean:release": "rimraf releases/src/",
    "copy:manifest": "cpx src/manifest.json releases/src/",
    "lint": "eslint '**/*.{js,ts,tsx}' --ignore-path .gitignore",
    "release": "run-s build && node scripts/zip.js",
    "start": "yarn watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "run-p watch:*",
    "watch:manifest": "cpx -w src/manifest.json dist/",
    "watch:contents": "webpack --watch --mode development"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.122",
    "@types/oauth": "^0.9.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "@types/twitter": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "cpx": "^1.5.0",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mz": "^2.7.0",
    "net": "^1.0.2",
    "node-twitter-api": "^1.8.0",
    "node-zip": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "oauth": "^0.9.15",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.1",
    "tls": "^0.0.1",
    "ts-loader": "^8.0.2",
    "twitter": "^1.7.1",
    "typescript": "^3.9.7",
    "webextension-polyfill-ts": "^0.19.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {}
}
