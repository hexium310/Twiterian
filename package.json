{
  "name": "Twiterian",
  "version": "0.2.1",
  "description": "Tweet with images at popup window.",
  "author": "Hexin",
  "license": "MIT",
  "repository": "https://github.com/hexium310/Twiterian",
  "browser": {
    "crypto": "./utils/hmac",
    "http": "stream-http",
    "https": "https-browserify",
    "stream": "stream-browserify"
  },
  "scripts": {
    "build": "run-s lint clean:release && tsc --noEmit && NODE_ENV=production node --require esbuild-register scripts/build.ts",
    "build:dev": "tsc --noEmit && NODE_ENV=development node --require esbuild-register scripts/build.ts",
    "clean": "run-p clean:*",
    "clean:dev": "rimraf dist/",
    "clean:release": "rimraf releases/src/",
    "lint": "eslint '**/*.{js,mjs,ts,tsx}' --ignore-path .gitignore",
    "release": "run-s build && node --experimental-json-modules scripts/zip.mjs",
    "start": "yarn watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "NODE_ENV=development NODE_WATCH=watch node --require esbuild-register scripts/build.ts"
  },
  "devDependencies": {
    "@chialab/esbuild-plugin-postcss": "^0.12.19",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@types/chrome": "^0.0.159",
    "@types/oauth": "^0.9.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "buffer": "^6.0.3",
    "cntl": "^1.0.0",
    "create-hmac": "^1.1.7",
    "esbuild": "^0.13.6",
    "esbuild-register": "^3.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.26.1",
    "events": "^3.3.0",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.1",
    "https-browserify": "^1.0.0",
    "jszip": "^3.7.1",
    "npm-run-all": "^4.1.5",
    "oauth": "^0.9.15",
    "postcss": "^8.3.9",
    "process": "^0.11.10",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^2.2.17",
    "twitter-api-client": "^1.3.8",
    "typescript": "^4.4.4",
    "url": "^0.11.0"
  },
  "dependencies": {}
}
