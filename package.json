{
  "name": "Twiterian",
  "version": "0.1.2",
  "description": "Tweet with images at popup window.",
  "author": "Hexin",
  "license": "MIT",
  "repository": "https://github.com/hexium310/Twiterian",
  "scripts": {
    "build": "run-s lint clean:release copy:manifest && webpack --mode production",
    "build:dev": "webpack --mode development && cpx src/manifest.json dist/",
    "clean": "run-p clean:*",
    "clean:dev": "rimraf dist/",
    "clean:release": "rimraf releases/src/",
    "copy:manifest": "cpx src/manifest.json releases/src/",
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json --formatters-dir node_modules/custom-tslint-formatters/formatters --format grouped",
    "release": "run-s build && node scripts/zip.js",
    "start": "yarn watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "run-p watch:*",
    "watch:manifest": "cpx -w src/manifest.json dist/",
    "watch:contents": "webpack --watch --mode development"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/chrome": "^0.0.75",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cpx": "^1.5.0",
    "css-loader": "^1.0.1",
    "custom-tslint-formatters": "^2.4.0",
    "eslint": "^5.8.0",
    "glob": "^7.1.3",
    "glob-promise": "^3.4.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mz": "^2.7.0",
    "net": "^1.0.2",
    "node-twitter-api": "^1.8.0",
    "node-zip": "^1.1.1",
    "npm-run-all": "^4.1.3",
    "query-string": "^6.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "svelte": "^2.15.2",
    "svelte-loader": "^2.11.0",
    "tls": "^0.0.1",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "twitter": "^1.7.1",
    "typescript": "^3.1.6",
    "webextension-polyfill-ts": "^0.8.9",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {}
}
